project.group = 'com.wdavilaneto'
project.name = 'wdavilaneto'


guick {

    installWebArtifacts {

        "archetype" {
            templates = [[input: "default/webapp", output: "src/main/webapp"]]
        }

        "resources" {
            templates = [
                    [input: "default/resources", output: "src/main/resources"],
                    [input: "default/testResources", output: "src/test/resources"]
            ]
        }

    }

}