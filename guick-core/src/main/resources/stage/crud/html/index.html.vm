#set( $entityBeanName = ${util.uncapitalize($entity.name)} )
#set( $projectBeanName = ${util.uncapitalize($project.name)} )
<!doctype html>
<html ng-app="${project.name}App" lang="pt">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title ng-scope>MPRJ - ${project.name}</title>
    <meta name="description" content="MPRJ - Módulo de Saúde Mental">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <!-- build:css(.) styles/vendor.css -->
    <!-- bower:css -->
    <link rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="bower_components/angular-ui-select/dist/select.css" />
    <link rel="stylesheet" href="bower_components/ng-grid/ng-grid.css" />
    <link rel="stylesheet" href="bower_components/angularjs-toaster/toaster.css" />
    <link rel="stylesheet" href="bower_components/angular-block-ui/dist/angular-block-ui.css" />
    <link rel="stylesheet" href="bower_components/fontawesome/css/font-awesome.css" />
    <link rel="stylesheet" href="bower_components/ionicons/css/ionicons.css" />
    <link rel="stylesheet" href="bower_components/nvd3/src/nv.d3.css" />
    <!-- endbower -->
    <!-- endbuild -->
    <!-- build:css(.tmp) styles/main.css -->
    <link rel="stylesheet" href="bower_components/jquery-ui/themes/smoothness/jquery-ui.css"/>
    <link rel="stylesheet" href="bower_components/adminlte/css/AdminLTE.css" />
    <link rel="stylesheet" href="bower_components/adminlte/css/font-awesome.css" />
    <link rel="stylesheet" href="bower_components/adminlte/css/ionicons.min.css" />
    <link rel="stylesheet" href="bower_components/angucomplete-alt/angucomplete-alt.css" />
    <link rel="stylesheet" href="css/main.css">
    <!-- endbuild -->
</head>
<body class="skin-blue">
<toaster-container></toaster-container>
<!--[if lt IE 7]>
<p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade
    your browser</a> to improve your experience.</p>
<![endif]-->


 <header class="header">
    <a href="#/" class="logo">
        <!-- Add the class icon to your logo image or logo icon to add the margining -->
        ${project.name}
    </a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top" role="navigation">
        <!-- Sidebar toggle button-->
        <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </a><a class="logo-control" href="#">#if (${project.name} == 'msm') MÓDULO DE SAÚDE MENTAL#else ${project.name} #end</a>
        <div class="navbar-right">
            <ul class="nav navbar-nav">
#foreach( ${entity} in ${project.getAllEntitiesWithDeadline()} )
                <li class="dropdown tasks-menu" ng-controller="${entity.beanName}AlertController">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <i class="fa fa-tasks"></i>
                        <span class="label label-danger" ng-if="alertCounter > 0">{{alertCounter}}</span>
                    </a>
                    <ul class="dropdown-menu" ng-if="alertCounter > 0">
                        <li class="header">Quantidade de ${entity.title} pendentes: {{alertCounter}} </li>
                        <li>
                            <!-- inner menu: contains the actual data -->
                            <ul class="menu" ng-repeat="${entity.beanName} in resultPage.content">
                                <!-- start message -->
                                <li>
##                                    <a href="#/">
                                        <p>{{${entity.beanName}.#if(${entity.getMostDescritiveProperties().size()}>0)${entity.getMostDescritiveProperties()[0]}#{else}id#{end}}} <small><i class="fa fa-clock-o"></i> {{${entity.beanName}.${entity.getAllDeadlineProperties()[0]}}}</small></p>
                                        <p>{{#if(${entity.getMostDescritiveProperties().size()}>1)${entity.getMostDescritiveProperties()[1]}#{else}id#{end}}}</p>
##                                    </a>
                                </li>
                                <!-- end message -->
                            </ul>
                        </li>
                        <li class="footer"><a href="#/${entity.name}">Ver Pendências</a></li>
                    </ul>
                </li>
#end
                <!-- User Account: style can be found in dropdown.less -->
                <li class="dropdown user user-menu"><a href="" class="dropdown-toggle" data-toggle="dropdown"> <i class="glyphicon glyphicon-user"></i> <span>Usuário MPRJ<i class="caret"></i></span></a>
                    <ul class="dropdown-menu">
                        <!-- User image -->
                        <li class="user-header bg-light-blue"><img src="bower_components/adminlte/img/avatar5.png" class="img-circle" alt="User Image">
                            <p>
                                Fulano<small>Usuário desde Nov. 2012</small>
                            </p></li>
                        <!-- Menu Body -->
                        <li class="user-body">
                            <div class="col-xs-4 text-center">
                                <a href="">Pendências</a>
                            </div>
                            <div class="col-xs-4 text-center">
                                <a href="">Principal</a>
                            </div>
                        </li>
                        <!-- Menu Footer-->
                        <li class="user-footer">
                            <div class="pull-left">
                                <a href="" class="btn btn-default btn-flat">Perfil</a>
                            </div>
                            <div class="pull-right">
                                <a href="" class="btn btn-default btn-flat">Sair</a>
                            </div>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </nav>
</header>

<div class="div.wrapper row-offcanvas row-offcanvas-left">
    <aside class="left-side sidebar-offcanvas">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
##            <!-- Sidebar user panel -->
##            <div class="user-panel">
##                <div class="pull-left info">
##                    <p>Olá, bem vindo!</p>
##                </div>
##            </div>

            <!-- sidebar menu: : style can be found in sidebar.less -->
            <ul class="sidebar-menu">

##                <li><a href="#/"><i class="glyphicon glyphicon-cog"></i>Atividades</a></li>
                <li><a href="#/dashboard"><i class="glyphicon glyphicon-signal"></i>Painel de Controle</a></li>
#if (${project.name} == 'msm')
                <li><a href="#/manterPaciente"><i class="fa fa-user"></i>Paciente</a></li>
                <li><a href="#/manterInstituicao"><i class="glyphicon glyphicon-plus-sign"></i>Instituição</a></li>
                <li><a href="#/manterInstituicao"><i class="glyphicon glyphicon-step-forward"></i>Movimentação</a></li>
#end

#if (${project.getAllMainEntities().size()} > 0)
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-group"></i> <span>Principal</span>
                        <i class="fa fa-angle-left pull-right"></i>
                    </a>
                    <ul class="treeview-menu">
#foreach( ${entity} in ${project.getAllMainEntities()})
                        <li><a href="#/${entity.name}"><i class="fa fa-angle-double-right"></i>${entity.title}</a></li>
#end
                    </ul>
                </li>
#end

#if (${project.getAllDomainEntities().size()} > 0)
                <li class="treeview">
                    <a href="#">
                        <i class="glyphicon glyphicon-wrench"></i> <span>Tabelas Dominio</span>
                        <i class="fa fa-angle-left pull-right"></i>
                    </a>
                    <ul class="treeview-menu">
#foreach( ${entity} in ${project.getAllDomainEntities()})
                        <li><a href="#/${entity.name}"><i class="fa fa-angle-double-right"></i>${entity.name}</a></li>
#end
                    </ul>
                </li>
#end

#if (${project.getAllEnumLikeEntities().size()} > 0)
                <li class="treeview">
                    <a href="#">
                        <i class="glyphicon glyphicon-wrench"></i> <span>Tipos</span>
                        <i class="fa fa-angle-left pull-right"></i>
                    </a>
                    <ul class="treeview-menu">
#foreach( ${entity} in ${project.getAllEnumLikeEntities()})
                        <li><a href="#/${entity.name}"><i class="fa fa-angle-double-right"></i>${entity.name}</a></li>
#end
                    </ul>
                </li>
#end

            </ul>
        </section>
        <!-- /.sidebar -->
    </aside>
    <div ng-include src="'views/modals.html'"></div>
    <aside class="right-side" id="home" ng-view></aside>
</div>

<!-- rodape <div ng-include src="'components/footer/footer.html'"></div>-->

